{
    "presets": [["es2015", { "loose": true }], "stage-1", "react"],
    "plugins": [
        ["transform-es2015-modules-commonjs", { "loose": true }],
        "add-module-exports",
        "transform-decorators-legacy",
        "transform-react-inline-elements",
        ["transform-runtime", {
            "polyfill": false,
            "regenerator": true
        }]
    ],
    "ignore": [
        "/node_modules/"
    ],
    "env": {
        "development": {
            "plugins": [

            ]
        }
    }
}
